# DockerHub MCP Server Configuration

# DockerHub Authentication (choose one method)
# Method 1: Username and Password
DOCKERHUB_USERNAME=your_username
DOCKERHUB_PASSWORD=your_password

# Method 2: Access Token (recommended)
# DOCKERHUB_TOKEN=your_access_token

# Private Registries (JSON array)
# PRIVATE_REGISTRIES='[{"name":"private-registry","url":"https://registry.example.com","username":"user","password":"pass","isDefault":false}]'

# Cache Configuration
CACHE_TTL=300
CACHE_CHECK_PERIOD=60
CACHE_MAX_KEYS=1000

# Rate Limiting Configuration
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000
RATE_LIMIT_BURST_SIZE=10

# Logging Configuration
LOG_LEVEL=info

# Server Configuration
NODE_ENV=production 