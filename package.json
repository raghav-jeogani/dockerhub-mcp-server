{
  "name": "dockerhub-mcp-server",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server for comprehensive DockerHub integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t dockerhub-mcp-server .",
    "docker:run": "docker run -it --rm dockerhub-mcp-server"
  },
  "keywords": [
    "mcp",
    "dockerhub",
    "docker",
    "ai",
    "model-context-protocol",
    "container-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^4.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
